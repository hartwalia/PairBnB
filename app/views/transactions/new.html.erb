<div class="form-container radius-box glassy-bg col-md-4 col-md-offset-4">
  <h2 class="mbs">Complete Reservation For <%= @reservation.listing.name %></h2>
  <%= form_tag reservation_transactions_path do%>
    <p>Please enter your payment details:</p>
    <div id="dropin"></div>
    <%=submit_tag "Pay $ #{@reservation.total_cost}", class: "button mt1" %>
  <%end%>
</div>
